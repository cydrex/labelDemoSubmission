<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Title</th>
      <th scope="col">Genre</th>
      <th scope="col">demo</th>
      <th scope ="col">Delete</th>
    </tr>
  </thead>
  
  <tbody>
    {{#each approved }}
      <tr>
          <th scope="row">{{ name }}</th>
          <td>{{ title }}</td>
          <td>{{ genre }}</td>
          <td><div class="player">
          </div><audio src="/{{demopath}}" preload="auto" /></td>
          <td><form method="POST" action="/delete/{{ _id }}"><button type="submit" class="btn btn-outline-danger waves-effect">Delete</button></form></td>
          </td>
      </tr>
    {{/each}}
  </tbody>
</table>



